	<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  
  <ViewSettingsDialog
    confirm="handleOdataRequestViewSettingDialogOKPress">
<!--     Sort Option -->
    <sortItems>
      <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_GWTOTAL}" key="gwtotal"/>
      <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_GWHUB}" key="gwhub" />
      <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_GWRFCOH}" key="gwrfcoh" />
      <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_GWBE}" key="gwbe" />
      <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_GWAPP}" key="gwapp" />
    </sortItems>
<!--     Filter Option -->
    <filterItems>
      <ViewSettingsFilterItem
        text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_FILTER}"
        key="Status"
        multiSelect="false">
        <items>
          <ViewSettingsItem text="{i18n>TXT_GENERIC_OK}" key="1" />
          <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_PENDING}" key="0" />
          <ViewSettingsItem text="{i18n>TXT_ODATAREQUESTCHECK_VIEWSETTINGSDIALOG_ERROR}" key="2" />
        </items>
      </ViewSettingsFilterItem>
    </filterItems>
  </ViewSettingsDialog>
</core:FragmentDefinition>
<ColumnListItem xmlns="sap.m" xmlns:core="sap.ui.core">
	<cells>
		<!--ChargeCode -->
		<Text id="chargeCodeText"
			tooltip="{i18n>TXT_GENERIC_CHARGE_CODE}" />

		<!-- Supplier -->
		<Text text="{SupplierID}"
			tooltip="{i18n>TXT_GENERIC_SUPPLIER}" />

		<!-- DeliveryLineNumber -->
		<Text id="deliveryNumberText"
			tooltip="{i18n>TXT_GENERIC_LINE}" />

		<!-- Quantity + UOM -->
		<Text id="quantityText"
			tooltip="{i18n>TXT_GENERIC_QUANTITY}" />

		<!-- Rate -->
		<Text id="rateText"
			tooltip="{i18n>TXT_ALLOCATION_EDITALLOCATIONS_CHARGECODE_COLUMN_RATE}" />

		<!-- Charges -currency -->
		<Text
			text="{parts : ['Charges', 'Currency'], formatter : 'com.zespri.awct.util.LocaleFormatHelper.formatAmount'}"
			tooltip="{i18n>TXT_ALLOCATION_EDITALLOCATIONS_CHARGECODE_COLUMN_CHARGES}" />
	</cells>
</ColumnListItem>